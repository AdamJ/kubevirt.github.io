---
layout: null
---
[
  {% for post in site.posts %}
    {
      "title"    : "<i class='fas fa-circle'></i> {{ post.title | strip_html | strip_newlines | remove_chars | escape }}",
      "category" : "{{ post.category }}",
      "tags"     : "{{ post.tags | join: ', ' }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}",
      "date"     : "{{ post.date }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
